package Modelo.UmPraUm;

public class Assento {

	private long Id;
	
	private String nome;

	public Assento() {
		
	}

	public Assento(long id, String nome) {
		super();
		Id = id;
		this.nome = nome;
	}

	public long getId() {
		return Id;
	}

	public void setId(long id) {
		Id = id;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
}
